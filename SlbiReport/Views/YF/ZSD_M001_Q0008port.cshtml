@{
    Layout = "~/Views/Shared/_LayoutEchart.cshtml";
}


@{

    ViewBag.Title = "新业务拓展情况统计分析";

}

<h2>@ViewBag.Title</h2>


    <div id="selectArea"></div>

    <div>
        <div id="bar" ng-controller="myBarCtrl" style="height: 400px;float:left;width:70%"></div>
        <div id="main" ng-controller="myCtrl" style="height: 400px;float:left;width:30%"></div>
    </div>


    <div>&nbsp;<table id="tt"  class="easyui-datagrid" ></table></div>

<script style="">
    $(window).load(function () {
        drawselect("selectArea", "SelZSD_M001_Q0008");
    drawbar2(null, "bar", "ZSD_M001_Q0009");
    drawpie2(null, "main", "ZSD_M001_Q0010");
    drawtable(null, "tt", "ZSD_M001_Q0008");
    });
    function buttononclick(pagequeryParams) {
        drawbar2(pagequeryParams, "bar", "ZSD_M001_Q0009");
        drawpie2(pagequeryParams, "main", "ZSD_M001_Q0010");
        drawtable(pagequeryParams, "tt", "ZSD_M001_Q0008");
    }
</script>